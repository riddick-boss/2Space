{
  "query": {
    "upcoming": false
  },
  "options": {
    "page": 1,
    "limit": 1,
    "pagination": true,
    "sort": {
      "flight_number": "desc"
    },
    "populate": [
      {
        "path": "cores"
      },
      {
        "path": "launchpad"
      },
      {
        "path": "rocket",
        "select": {
          "name": 1
        }
      },
      {
        "path": "fairings"
      },
      {
        "path": "capsules"
      },
      {
        "path": "payloads"
      },
      {
        "path": "crew",
        "populate": [
          {
            "path": "crew"
          }
        ]
      },
      {
        "path": "cores"
      }
    ]
  }
}